<web-app>
    <servlet>
        <servlet-name>usuarios</servlet-name>
        <servlet-class>gestionUsuarios.controlador.AdministracionUsuarios</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>usuarios</servlet-name>
        <url-pattern>/users</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>gestionUsuarios.controlador.ControladorLogin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>registro</servlet-name>
        <servlet-class>gestionUsuarios.controlador.Registro</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>registro</servlet-name>
        <url-pattern>/registro</url-pattern>
    </servlet-mapping>

	<servlet>
		<servlet-name>autocompletado</servlet-name>
        <servlet-class>otros.Autocompletado</servlet-class>
	</servlet>
    <servlet-mapping>
        <servlet-name>autocompletado</servlet-name>
        <url-pattern>/autocompletado</url-pattern>
    </servlet-mapping>
</web-app>
